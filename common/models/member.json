{
  "name": "Member",
  "plural": "members",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "avatar": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "news": {
      "type": "hasMany",
      "model": "News",
      "foreignKey": "memberId"
    },
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": "memberId"
    },
    "likesApartments": {
      "type": "hasMany",
      "model": "LikeApartments",
      "foreignKey": "memberId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [],
  "methods": {}
}
