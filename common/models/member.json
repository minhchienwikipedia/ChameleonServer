{
  "name": "Member",
  "plural": "members",
  "base": "User",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "avatar": {
      "type": "string"
    },
    "memberId": {
      "type": "string",
      "id": true,
      "generated": true
    }
  },
  "validations": [],
  "relations": {
    "news": {
      "type": "hasMany",
      "model": "News",
      "foreignKey": "memberId"
    },
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": "memberId"
    },
    "likesApartments": {
      "type": "hasMany",
      "model": "LikeApartments",
      "foreignKey": "memberId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "memberId"
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
