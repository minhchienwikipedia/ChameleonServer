  "options": {
    "validateUpsert": true
  },
  "properties": {
    "newsId": {
      "type": "number",
      "id": true,
      "generated": false,
      "required": false
    },
    "image": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "time": {
      "type": "date"
    },
    "status": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
